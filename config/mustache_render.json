{
    "production": {
        "options": {
            "escape": false
        },
        "files": [
            {
                "data": "mustache-prod.json",
                "template": "index.mustache",
                "dest": "public/index.html"
            }
        ]
    },
    "staging": {
        "options": {
            "escape": false
        },
        "files": [
            {
                "data": "mustache-stage.json",
                "template": "index.mustache",
                "dest": "public/index.html"
            }
        ]
    },
    "development": {
        "options": {
            "escape": false
        },
        "files": [
            {
                "data": "mustache-local.json",
                "template": "index.mustache",
                "dest": "public/index.html"
            }
        ]
    }
}
